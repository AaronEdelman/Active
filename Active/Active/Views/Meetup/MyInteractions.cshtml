@model Active.Models.MyInteractionsViewModel

@{
    ViewBag.Title = "MyInteractions";
}

<h2>MyInteractions</h2>

@foreach(var interaction in Model.Interactions)
{
    @Html.DisplayFor(modelItem => interaction.User.FirstName)
    @Html.DisplayFor(modelItem => interaction.Activity.Name)
    @Html.DisplayFor(modelItem => interaction.Activity.TimeStart)
    <button id="button1" onclick="location.href='@Url.Action("Rate", "Meetup", new { id = 0 })'">No Show</button>
    <button id="button2" onclick="location.href='@Url.Action("Rate", "Meetup", new { id = 1 })'">Garbage Person</button>
    <button id="button3" onclick="location.href='@Url.Action("Rate", "Meetup", new { id = 2 })'">Not Great</button>
    <button id="button4" onclick="location.href='@Url.Action("Rate", "Meetup", new { id = 3 })'">So-so</button>
    <button id="button5" onclick="location.href='@Url.Action("Rate", "Meetup", new { id = 4 })'">Good</button>
    <button id="button6" onclick="location.href='@Url.Action("Rate", "Meetup", new { id = 5 })'">Awesome</button>
}


